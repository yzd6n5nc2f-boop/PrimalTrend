{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/*.{css,scss,js,json}", "/*.{png,jpg,jpeg,gif,svg}"]
  },
  "globalHeaders": {
    "Cache-Control": "public, max-age=31536000, immutable"
  },
  "routes": [
    {
      "route": "/_next/static/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/placeholders/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
